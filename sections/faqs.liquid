<div id="faq" class="padding-global">
   <div class="container-large">
      <div class="padding-section-medium">
         <div class="faq4_component">
            <div class="settings-schema w-embed"></div>
            <div class="margin-bottom margin-xlarge">
               <div class="text-align-center">
                  <div class="max-width-medium align-center">
                     <h2 li-object="section.settings.faq_heading_text" class="heading-style-h2">{{ section.settings.faq_heading_text }}</h2>
                  </div>
               </div>
            </div>
            <div class="max-width-large align-center">
               <div class="w-layout-grid faq4_list">
                  {% for faq in metaobjects.faq.values %}    
                  <div li-for="faq in metaobjects.faq.values" class="faq4_accordion">
                     <div class="faq4_question">
                        <div li-object="faq.question" class="text-size-medium text-weight-bold">{{ faq.question }}</div>
                        <div class="faq4_icon-wrappper">
                           <div class="button is-xsmall">
                              <svg xmlns="http://www.w3.org/2000/svg" viewbox="0 0 448 512" width="100%" fill="currentColor" class="icon-1x1-small">
                                 <path d="M248 72c0-13.3-10.7-24-24-24s-24 10.7-24 24l0 160L40 232c-13.3 0-24 10.7-24 24s10.7 24 24 24l160 0 0 160c0 13.3 10.7 24 24 24s24-10.7 24-24l0-160 160 0c13.3 0 24-10.7 24-24s-10.7-24-24-24l-160 0 0-160z"></path>
                              </svg>
                           </div>
                        </div>
                     </div>
                     <div class="faq4_answer">
                        <div class="margin-bottom margin-small">
                           <div class="richtext-wrapper w-richtext">
                              <p li-object="faq.answer">{{ faq.answer }}</p>
                           </div>
                        </div>
                     </div>
                  </div>
                  {% endfor %}
                  <div li-for="faq in metaobjects.faq.values" class="faq4_accordion">
                     <div class="faq4_question">
                        <div li-object="faq.question" class="text-size-medium text-weight-bold">{{ section.settings.faq_cta_heading_text }}</div>
                        <div class="">
                           {% render 'button' %}
                        </div>
                     </div>
                     
                  </div>
               </div>
            </div>
            {% comment %} <div class="margin-top margin-xxlarge">
               <div class="text-align-center">
                  <div class="max-width-medium align-center">
                     <div class="margin-bottom margin-xsmall">
                        <h3 li-object="section.settings.faq_cta_heading_text">{{ section.settings.faq_cta_heading_text }}</h3>
                     </div>
                     <div class="richtext-wrapper w-richtext">
                        <p li-object="section.settings.faq_cta_paragraph_text">{{ section.settings.faq_cta_paragraph_text }}</p>
                     </div>
                     <div class="margin-top margin-small">
                        <div class="button-group is-center">{% render 'button' %}</div>
                     </div>
                  </div>
               </div>
            </div> {% endcomment %}
         </div>
      </div>
   </div>
</div>
<style>
    .faq4_component {
        display: flex;
    }
    @media screen and (max-width: 767px) {
        .faq4_component {
            display: block;
        }
    }
