<style>
/* Mobile accordion styles */
@media (max-width: 768px) {
  .footer16_menu-wrapper {
    display: block !important;
  }
  
  .footer16_accordion-column {
    width: 100% !important;
    grid-column: 1 / -1 !important;
    margin-bottom: 0;
  }
  
  .footer16_accordion-column .footer16_accordion-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 15px 0;
    border-bottom: 1px solid rgba(255, 255, 255, 0.8);
    cursor: pointer;
  }
  
  .footer16_accordion-column .footer16_accordion-icon {
    width: 32px;
    height: 32px;
    border-radius: 50%;
    background: #F66718;
    display: flex;
    align-items: center;
    justify-content: center;
    font-size: 18px;
    font-weight: bold;
    color: white;
    transition: transform 0.3s ease;
  }
  
  .footer16_accordion-column.active .footer16_accordion-icon {
    transform: rotate(45deg);
  }
  
  .footer16_accordion-column .footer16_accordion-content {
    max-height: 0;
    overflow: hidden;
    transition: max-height 0.3s ease;
  }
  
  .footer16_accordion-column.active .footer16_accordion-content {
    max-height: 500px;
    padding-bottom: 15px;
  }
  
  .footer16_accordion-column .margin-bottom {
    display: none;
  }
}

/* Desktop styles - normal display */
@media (min-width: 769px) {
  .footer16_accordion-header {
    cursor: default;
  }
  
  .footer16_accordion-icon {
    display: none;
  }
  
  .footer16_accordion-content {
    max-height: none !important;
    overflow: visible !important;
  }
  
  .footer16_accordion-column .margin-bottom {
    display: block;
  }
}
</style>

<script>
function toggleFooterAccordion(header) {
  // Only work on mobile screens
  if (window.innerWidth <= 768) {
    const column = header.parentElement;
    const isActive = column.classList.contains('active');
    
    // Close all accordions first
    document.querySelectorAll('.footer16_accordion-column').forEach(col => {
      col.classList.remove('active');
    });
    
    // Toggle current accordion
    if (!isActive) {
      column.classList.add('active');
    }
  }
}

// Initialize accordions on page load
document.addEventListener('DOMContentLoaded', function() {
  // Close all accordions by default on mobile
  if (window.innerWidth <= 768) {
    document.querySelectorAll('.footer16_accordion-column').forEach(col => {
      col.classList.remove('active');
    });
  }
});

// Handle window resize
window.addEventListener('resize', function() {
  if (window.innerWidth > 768) {
    // Remove active class on desktop
    document.querySelectorAll('.footer16_accordion-column').forEach(col => {
      col.classList.remove('active');
    });
  }
});
</script>